<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.fjfhlqjtg.log.dao.LogDao">

	<select id="queryForSaleSys" parameterType="String" resultType="String">
		select c_srch_val from t_qrypara where c_srch_cde = 'XSTX' AND c_srch_val=#{saleSys}
	</select>
</mapper> 
