<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.fjfhlqjtg.log.dao.LogDao">

	<insert id="saveLog" parameterType="com.fjfhlqjtg.log.vo.LogVo">
		insert into t_p_log_interface
		t(t_crt_tm,c_ply_app_no,c_ply_no,c_edr_app_no,c_edr_no,c_clm_no,c_undr_mrk,n_clm_tms,c_interface_nme,c_interface_content)
		values
		(#{t_crt_tm},#{c_ply_app_no},#{c_ply_no},#{c_edr_app_no},#{c_edr_no},#{c_clm_no},#{c_undr_mrk},#{n_clm_tms},#{c_interface_nme},#{c_interface_content})
	</insert>

</mapper> 
